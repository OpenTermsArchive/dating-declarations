{
  "name": "Jdate",
  "terms": {
    "Terms of Service": {
      "fetch": "https://about.jdate.com/legal-en/terms-of-use/",
      "select": ".entry-content"
    },
    "Privacy Policy": {
      "fetch": "https://about.jdate.com/legal-en/privacy-policy/",
      "select": [
        ".legal-text-container:nth-child(1)",
        ".entry-content"
      ],
      "remove": [
        "#CookieNotice"
      ]
    },
    "Trackers Policy": {
      "fetch": "https://about.jdate.com/legal-en/privacy-policy/",
      "select": {
        "startBefore": "#cookiepolicy",
        "endBefore": ".entry-footer"
      }
    }
  }
}
